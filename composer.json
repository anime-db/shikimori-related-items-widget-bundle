{
	"name" : "anime-db/shikimori-related-items-widget-bundle",
	"license" : "GPL-3.0",
	"type" : "anime-db-plugin",
	"description" : "Plugin Shikimori for the catalog Anime DB",
	"homepage" : "http://github.com/anime-db/shikimori-related-items-widget-bundle",
	"autoload" : {
		"psr-4" : {
			"AnimeDb\\Bundle\\ShikimoriRelatedItemsWidgetBundle\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"AnimeDb\\Bundle\\ShikimoriRelatedItemsWidgetBundle\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=5.4.0",
		"anime-db/catalog-bundle" : ">=0.4.17",
		"anime-db/shikimori-widget-bundle" : ">=0.1.1"
	},
	"require-dev" : {
		"anime-db/shikimori-filler-bundle" : ">=0.1.9"
	},
	"suggest" : {
		"anime-db/shikimori-filler-bundle" : "Allows to add links to the filling items"
	},
	"minimum-stability" : "dev"
}
